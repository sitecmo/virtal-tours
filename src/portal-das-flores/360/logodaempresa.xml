<krpano version="1.0.8" logkey="true">

	<plugin name="data" keep="true"/>
	
    <plugin name="logo-empresa" 
			align="righttop" 
			keep="true"
			alpha="0.7"
			onover="tween(alpha,1,0.1);"
			onout="tween(alpha,0.7,0.7);"
			url="%SWFPATH%/imgtemplate/logodaempresa.png"
			onclick="openurl('http://ambconstrutora.com.br/hot_site/novo/', _blank);"
			x="10" y="55"
			zorder="999"
	/>

	<plugin name="telefone"
			url="%SWFPATH%/imgtemplate/textfield.swf"
			devices="all"
			align="lefttop"
			keep="true"
			width="100" height="50"
			x="30" y="100"
			html="[p](62) 3942.3424[/p]"
			css="p{color:#000000; font-family:verdana; font-weight:regular; font-size:9; margin-left:5; margin-right:5; text-align:center; }"
			backgroundcolor="0xededed" roundedge="5" shadow="1" borderwidth="0" glow="4" glowcolor="0xFFFFFF"
			autosize="center"
			alpha="0.70"
			zorder="200"
	/>
	
</krpano>